<?xml version="1.0" encoding="utf-8" ?>
 <hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
  <class name="TE.Net.Common.Core.Entity.TipoProduto, TE.Net.Common.Core" lazy="false" table="tipo_usuario">
    <id name="Id" column="id">
      <generator class="sequence">
        <param name="sequence">tipo_produto_id_seq</param>
      </generator>
    </id>
    
    <property name="Nome" type="String" column="nome"/>
    <property name="Descricao" type="String" column="descricao"/>
  </class>
</hibernate-mapping>  
